import requests

from bs4 import BeautifulSoup

def getInfo(drug):
    try:

        input = str(drug)
        url = "https://psychonautwiki.org/wiki/"+input+"/Summary"
        html = requests.get(url)
        htmlinfo = BeautifulSoup(html.text, 'html.parser')

        print(htmlinfo)
    except:
        return('fail')

    


